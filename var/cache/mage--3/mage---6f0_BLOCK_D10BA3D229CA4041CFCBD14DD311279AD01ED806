<script type="text/javascript">
	require(['jquery'],function($){
		jQuery(document).ready(function($) {
			$("#menu-top15609066741189956171 .dynamic-items li:first").addClass("dynamic-active");
			var id = $("#menu-top15609066741189956171 .dynamic-items li:first").data("dynamic-id");
			$("#menu-top15609066741189956171 ."+id).addClass("dynamic-active");
			$("#menu-top15609066741189956171 .level0 .submenu").find(".active").addClass("");

			$("#menu-top15609066741189956171").find(".level0 .submenu").each(function(index, elemetn){
				var eSelected = $(this).find(".active");
				if(eSelected.length){
					$(this).addClass("active");
				}
			});
		});

		$("#menu-top15609066741189956171 .dynamic-items li").hover(function(){
			$(this).parents(".dynamic-items").find("li").removeClass("dynamic-active");
			$(this).addClass("dynamic-active");
			var id = $(this).data("dynamic-id");
			$("#menu-top15609066741189956171 ."+id).parent().find(".dynamic-item").removeClass("dynamic-active");
			$("#menu-top15609066741189956171 ."+id).addClass("dynamic-active");
		});

		$("#menu-top15609066741189956171 li a").hover(function(){
			$(this).css({
				"background-color": $(this).data("hover-bgcolor"),
				"color": $(this).data("hover-color")
			});
		}, function(){
			$(this).css({
				"background-color": $(this).data("bgcolor"),
				"color": $(this).data("color")
			});
		});
		$(window).on("resize", function(){
			if($("#menu-top15609066741189956171").data("disable-bellow")){
				if ($(window).width() <= $("#menu-top15609066741189956171").data("disable-bellow")){
					$("#menu-top15609066741189956171-menu").hide();
				}else{
					$("#menu-top15609066741189956171-menu").show();
				}
			}
			$("#menu-top15609066741189956171").find("li").each(function(index, element){
				if($(this).data("disable-bellow") && $(window).width() <= $(this).data("disable-bellow")){
					$(this).addClass("hidden");
				}else if($(this).hasClass("hidden")){
					$(this).removeClass("hidden");
				}
			});
			if ($(window).width() <= 999){
				$("#menu-top15609066741189956171").addClass("nav-mobile");
			}else{
				$("#menu-top15609066741189956171").find("div").removeClass("mbactive");
				$("#menu-top15609066741189956171").removeClass("nav-mobile");
			}
		}).resize();
		//Toggle mobile menu
		$('#menu-top15609066741189956171 .opener').on('click', function(e) {
			e.preventDefault();
			$("#menu-top15609066741189956171 .nav-item").removeClass("item-active");
			var parent = $(this).parents(".nav-item").eq(0);
			$(this).toggleClass('item-active');
			$(parent).find(".submenu").eq(0).slideToggle();
			return false;
		});

		var menuToogle = function () {
			if ($('html').hasClass('nav-open')) {
				$('html').removeClass('nav-open');
				setTimeout(function () {
					$('html').removeClass('nav-before-open');
				}, 300);
			} else {
				$('body').append('<div class="ves-overlay ves-overlaymenu-top15609066741189956171"></div>');
				$('html').addClass('nav-before-open');
				setTimeout(function () {
					$('html').addClass('nav-open');
				}, 42);
			}
		}
		$(document).on("click", ".action.nav-toggle", menuToogle);
		$(document).on("click", ".ves-overlaymenu-top15609066741189956171", function(){
		    $('#menu-top15609066741189956171').css("left","");
		    $('html').removeClass('nav-open');
		    setTimeout(function () {
		     	$('html').removeClass('nav-before-open');
		    }, 300);
		    $('.ves-overlay').remove();
		    return false;
		});

	});
</script>
