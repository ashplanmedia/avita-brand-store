				$("#menu-vertical1560758177556851905 .dynamic-items li:first").addClass("dynamic-active");
				var id = $("#menu-vertical1560758177556851905 .dynamic-items li:first").data("dynamic-id");
				$("#menu-vertical1560758177556851905 ."+id).addClass("dynamic-active");
