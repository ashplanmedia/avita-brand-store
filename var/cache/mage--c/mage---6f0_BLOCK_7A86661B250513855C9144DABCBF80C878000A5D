			jQuery(document).ready(function($) {
				$("#menu-vertical1559381593994730272 .dynamic-items li:first").addClass("dynamic-active");
				var id = $("#menu-vertical1559381593994730272 .dynamic-items li:first").data("dynamic-id");
				$("#menu-vertical1559381593994730272 ."+id).addClass("dynamic-active");
				$("#menu-vertical1559381593994730272 .level0 .submenu").find(".active").addClass("");

				$("#menu-vertical1559381593994730272").find(".level0 .submenu").each(function(index, elemetn){
					var eSelected = $(this).find(".active");
					if(eSelected.length){
						$(this).addClass("active");
					}
				});
			});

			$("#menu-vertical1559381593994730272 .dynamic-items li").hover(function(){
				$(this).parents(".dynamic-items").find("li").removeClass("dynamic-active");
				$(this).addClass("dynamic-active");
				var id = $(this).data("dynamic-id");
				$("#menu-vertical1559381593994730272 ."+id).parent().find(".dynamic-item").removeClass("dynamic-active");
				$("#menu-vertical1559381593994730272 ."+id).addClass("dynamic-active");
			});

			$("#menu-vertical1559381593994730272 li a").hover(function(){
				$(this).css({
					"background-color": $(this).data("hover-bgcolor"),
					"color": $(this).data("hover-color")
				});
			}, function(){
				$(this).css({
					"background-color": $(this).data("bgcolor"),
					"color": $(this).data("color")
				});
			});
			$(window).on("resize", function(){
				if($("#menu-vertical1559381593994730272").data("disable-bellow")){
					if ($(window).width() <= $("#menu-vertical1559381593994730272").data("disable-bellow")){
						$("#menu-vertical1559381593994730272-menu").hide();
					}else{
						$("#menu-vertical1559381593994730272-menu").show();
					}
				}
				$("#menu-vertical1559381593994730272").find("li").each(function(index, element){
					if($(this).data("disable-bellow") && $(window).width() <= $(this).data("disable-bellow")){
						$(this).addClass("hidden");
					}else if($(this).hasClass("hidden")){
						$(this).removeClass("hidden");
					}
				});
				if ($(window).width() <= 974){
					$("#menu-vertical1559381593994730272").addClass("nav-mobile");
				}else{
					$("#menu-vertical1559381593994730272").find("div").removeClass("mbactive");
					$("#menu-vertical1559381593994730272").removeClass("nav-mobile");
				}
			}).resize();
		//Toggle mobile menu
		$('#menu-vertical1559381593994730272 .opener').on('click', function(e) {
			e.preventDefault();
			$("#menu-vertical1559381593994730272 .nav-item").removeClass("item-active");
			var parent = $(this).parents(".nav-item").eq(0);
			$(this).toggleClass('item-active');
			$(parent).find(".submenu").eq(0).slideToggle();
			return false;
		});
		
		$(document).on('click', function(e){
			e.stopPropagation();
			if ($('body').hasClass('ves-nav-open')) {
				$('body').removeClass('ves-nav-open');
				setTimeout(function () {
					$('body').removeClass('ves-nav-before-open');
				}, 300);
			}
		});

		$(document).on("click", ".menu-vertical1559381593994730272", function(){
			$('body').removeClass('ves-offcanvas-menu-left');
			if ($('body').hasClass('ves-nav-open')) {
				$('body').removeClass('ves-nav-open');
				setTimeout(function () {
					$('body').removeClass('ves-nav-before-open');
					$('body').removeClass('');
				}, 300);
			} else {
				$('body').addClass('ves-nav-before-open');
				setTimeout(function () {
					$('body').addClass('ves-nav-open');
					$('body').addClass('');
				}, 42);
			}
			return false;
		});
