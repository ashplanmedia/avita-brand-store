					var eSelected = $(this).find(".active");
					if(eSelected.length){
						$(this).addClass("active");
					}
				});
			});

			$("#menu-vertical1560758177556851905 .dynamic-items li").hover(function(){
				$(this).parents(".dynamic-items").find("li").removeClass("dynamic-active");
				$(this).addClass("dynamic-active");
				var id = $(this).data("dynamic-id");
				$("#menu-vertical1560758177556851905 ."+id).parent().find(".dynamic-item").removeClass("dynamic-active");
				$("#menu-vertical1560758177556851905 ."+id).addClass("dynamic-active");
			});

			$("#menu-vertical1560758177556851905 li a").hover(function(){
				$(this).css({
