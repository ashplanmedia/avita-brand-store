			$(window).on("resize", function(){
				if($("#menu-vertical15605128301875097372").data("disable-bellow")){
					if ($(window).width() <= $("#menu-vertical15605128301875097372").data("disable-bellow")){
						$("#menu-vertical15605128301875097372-menu").hide();
					}else{
						$("#menu-vertical15605128301875097372-menu").show();
					}
				}
				$("#menu-vertical15605128301875097372").find("li").each(function(index, element){
					if($(this).data("disable-bellow") && $(window).width() <= $(this).data("disable-bellow")){
						$(this).addClass("hidden");
					}else if($(this).hasClass("hidden")){
						$(this).removeClass("hidden");
					}
				});
				if ($(window).width() <= 974){
					$("#menu-vertical15605128301875097372").addClass("nav-mobile");
				}else{
					$("#menu-vertical15605128301875097372").find("div").removeClass("mbactive");
					$("#menu-vertical15605128301875097372").removeClass("nav-mobile");
				}
			}).resize();
		//Toggle mobile menu
		$('#menu-vertical15605128301875097372 .opener').on('click', function(e) {
			e.preventDefault();
			$("#menu-vertical15605128301875097372 .nav-item").removeClass("item-active");
			var parent = $(this).parents(".nav-item").eq(0);
			$(this).toggleClass('item-active');
			$(parent).find(".submenu").eq(0).slideToggle();
			return false;
		});
		
		$(document).on('click', function(e){
			e.stopPropagation();
			if ($('body').hasClass('ves-nav-open')) {
				$('body').removeClass('ves-nav-open');
				setTimeout(function () {
					$('body').removeClass('ves-nav-before-open');
				}, 300);
			}
		});

		$(document).on("click", ".menu-vertical15605128301875097372", function(){
			$('body').removeClass('ves-offcanvas-menu-left');
			if ($('body').hasClass('ves-nav-open')) {
				$('body').removeClass('ves-nav-open');
				setTimeout(function () {
					$('body').removeClass('ves-nav-before-open');
					$('body').removeClass('');
				}, 300);
			} else {
				$('body').addClass('ves-nav-before-open');
				setTimeout(function () {
					$('body').addClass('ves-nav-open');
					$('body').addClass('');
				}, 42);
			}
			return false;
		});
	});</script></div></div></div></div></div></div></div></div></div><div class="ves-container   "><div><div class="ves-inner   "><div class="row row-level-1  "><div class="row-inner clearfix "><div class=" col-lg-12 col-md-12 col-sm-12 col-xs-12  "><div class="col-inner "><div><div id="productlist-7381397661560512830" class="block widget ves-widget productlist-widget productlist-easytab-play "><div id="easytab-7381397661560512830" class="block-content" data-animate="true" data-animation-speed="0" data-collapsible="false" data-cycle="false" data-transition-in="fadeIn" data-transition-in-easing="swing" data-transition-out="fadeOut" data-transition-out-easing="swing" data-event="click"><ul class="etabs"><li class="nav-item tab " id="tab-productlist-featured-366079781560512830"><a href="#productlist-featured-366079781560512830">Featured</a></li></ul><div class="tab-content productlist-owlcarousel-play" id="productlist-owlcarousel-7381397661560512830" data-nav="false" data-dot="false" data-autoplay="false" data-autoplay-timeout="3000" data-mobile-items="1" data-tablet-small-items="2" data-tablet-items="2" data-portrait-items="1" data-large-items="1" data-large-max-items="1" data-rtl="false" data-loop="false" data-lazyload="true" data-nav-text-owlpre="" data-nav-text-owlnext=""><div id="productlist-featured-366079781560512830" class="ves-products-grid grid  "><div class="owl-carousel product-items"><div class="product-block"><div class="product-item-info"><div class="product-item-name"> <a class="product-item-link" href="https://store.avita-india.com/index.php/avita-liber-core-i7-8th-gen-8gb-ram-256gb-ssd-14-inch-fhd-windows-10-home-ns14a2in289p-thin-and-light-laptop-snow-flakes-1-46kg.html">AVITA LIBER (Core i7 - 8th Gen / 8GB RAM / 256GB SSD / 14-inch FHD / Windows 10 Home) NS14A2IN289P Thin and Light Laptop (Snow Flakes, 1.46Kg)</a></div><div class="product-image"><div class="product-img"><a href="https://store.avita-india.com/index.php/avita-liber-core-i7-8th-gen-8gb-r